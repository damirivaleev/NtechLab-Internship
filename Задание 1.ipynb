      "source": [
        "def findMaxSubArray(a):\n",
        "    max = min(a)\n",
        "    for i, x in enumerate(a):\n",
        "        for j in range(1, len(a) - i + 1):\n",
        "            if sum(a[i:i + j]) >= max:\n",
        "                res = a[i:i + j]\n",
        "                max = sum(res)\n",
        "\n",
        "    return res"
      ],
